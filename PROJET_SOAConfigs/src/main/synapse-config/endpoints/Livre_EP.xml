<?xml version="1.0" encoding="UTF-8"?>
<endpoint name="Livre_EP" xmlns="http://ws.apache.org/ns/synapse">
    <http method="get" uri-template="http://localhost:3000/livre/{uri.var.qr}">
        <suspendOnFailure>
            <initialDuration>-1</initialDuration>
            <progressionFactor>1.0</progressionFactor>
        </suspendOnFailure>
        <markForSuspension>
            <retriesBeforeSuspension>0</retriesBeforeSuspension>
        </markForSuspension>
    </http>
</endpoint>
